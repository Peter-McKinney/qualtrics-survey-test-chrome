{
  "name": "qualtrics-survey-test",
  "description": "Clears browser storage for a given intercept survey and launches the survey using window['QSI'].API.run()",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "cookies"],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["styles.css", "index.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "reloader.js"
  }
}
